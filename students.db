-- إنشاء قاعدة البيانات
CREATE DATABASE IF NOT EXISTS students_db;

-- استخدام قاعدة البيانات
USE students_db;

-- إنشاء جدول الطلاب
CREATE TABLE IF NOT EXISTS students (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    grade TEXT NOT NULL
);

